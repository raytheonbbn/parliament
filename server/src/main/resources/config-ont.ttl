# baseURI: http://parliament.semwebcentral.org/parliament-config
# prefix: pcfg

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pcfg: <http://parliament.semwebcentral.org/parliament-config#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://parliament.semwebcentral.org/parliament-config>
  a owl:Ontology ;
  rdfs:label "Parliament Model Configuration Ontology" ;
  owl:versionInfo "1.1" ;
.
pcfg:Configuration
  a owl:Class ;
  rdfs:label "Configuration" ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom pcfg:ConfigurationHandler ;
      owl:onProperty pcfg:configurationHandler ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom pcfg:IndexProcessorConfigurationHandler ;
      owl:onProperty pcfg:indexHandler ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty pcfg:clearDataOnStartup ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:integer ;
      owl:onProperty pcfg:deferredFileOutputStreamThreshold ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty pcfg:baselineDir ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty pcfg:clearDataOnStartup ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty pcfg:indexHandler ;
    ] ;
.
pcfg:ConfigurationHandler
  a owl:Class ;
  rdfs:label "Configuration Handler" ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty pcfg:implentationClass ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality 1 ;
      owl:onProperty pcfg:implentationClass ;
    ] ;
.
pcfg:IndexProcessorConfiguration
  a owl:Class ;
  rdfs:label "Index Processor Configuration" ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty pcfg:processorClass ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality 1 ;
      owl:onProperty pcfg:processorClass ;
    ] ;
.
pcfg:IndexProcessorConfigurationHandler
  a owl:Class ;
  pcfg:implementationClass "com.bbn.parliament.server.configuration.IndexProcessorConfigurationHandler" ;
  rdfs:label "Index Processor Configuration Handler" ;
  rdfs:subClassOf pcfg:ConfigurationHandler ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom pcfg:IndexProcessorConfiguration ;
      owl:onProperty pcfg:index ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality 1 ;
      owl:onProperty pcfg:index ;
    ] ;
.
pcfg:JenaRuleConfigurationHandler
  a owl:Class ;
  pcfg:implementationClass "com.bbn.parliament.server.configuration.JenaRuleConfigurationHandler" ;
  rdfs:label "Jena Rule Configuration Handler" ;
  rdfs:subClassOf pcfg:RuleConfigurationHandler ;
.
pcfg:MemoryConfigurationHandler
  a owl:Class ;
  pcfg:implementationClass "com.bbn.parliament.server.configuration.MemoryConfigurationHandler" ;
  rdfs:label "Memory Configuration Handler" ;
  rdfs:subClassOf pcfg:ConfigurationHandler ;
.
pcfg:RuleConfigurationHandler
  a owl:Class ;
  rdfs:label "Rule Configuration Handler" ;
  rdfs:subClassOf pcfg:ConfigurationHandler ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality 1 ;
      owl:onProperty pcfg:ruleFile ;
    ] ;
.
pcfg:baselineDir
  a owl:DatatypeProperty ;
.
pcfg:clearDataOnStartup
  a owl:DatatypeProperty ;
.
pcfg:configurationHandler
  a owl:ObjectProperty ;
.
pcfg:deferredFileOutputStreamThreshold
  a owl:DatatypeProperty ;
.
pcfg:enabledByDefault
  a owl:DatatypeProperty ;
.
pcfg:implementationClass
  a owl:DatatypeProperty ;
.
pcfg:index
  a owl:ObjectProperty ;
.
pcfg:indexHandler
  a owl:ObjectProperty ;
.
pcfg:prefix
  a owl:DatatypeProperty ;
.
pcfg:prefixes
  a owl:ObjectProperty ;
.
pcfg:processorFactoryClass
  a owl:DatatypeProperty ;
.
pcfg:ruleFile
  a owl:ObjectProperty ;
.
pcfg:uri
  a owl:DatatypeProperty ;
.
pcfg:usageThreshold
  a owl:DatatypeProperty ;
.
