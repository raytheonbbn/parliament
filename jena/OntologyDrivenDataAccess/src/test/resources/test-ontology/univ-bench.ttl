ub:TechnicalReport
  a owl:Class ;
  rdfs:label "technical report" ;
  rdfs:subClassOf ub:Article ;
.
ub:UndergraduateStudent
  a owl:Class ;
  rdfs:label "undergraduate student" ;
  rdfs:subClassOf ub:Student ;
.
ub:University
