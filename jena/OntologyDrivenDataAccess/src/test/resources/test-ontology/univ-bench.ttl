.
ub:Dean
  a owl:Class ;
  rdfs:label "dean" ;
  rdfs:subClassOf ub:Professor ;
  owl:intersectionOf (
      ub:Person
      [
        a owl:Restriction ;
        owl:onProperty ub:headOf ;
        owl:someValuesFrom ub:College ;
      ]
    ) ;
.
ub:Department
  a owl:Class ;
  rdfs:label "university department" ;
  rdfs:subClassOf ub:Organization ;
.
ub:Director
  a owl:Class ;
  rdfs:label "director" ;
  owl:intersectionOf (
      ub:Person
      [
        a owl:Restriction ;
        owl:onProperty ub:headOf ;
        owl:someValuesFrom ub:Program ;
      ]
    ) ;
