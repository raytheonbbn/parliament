  a owl:Class ;
  rdfs:label "student" ;
  owl:intersectionOf (
      ub:Person
      [
        a owl:Restriction ;
        owl:onProperty ub:takesCourse ;
        owl:someValuesFrom ub:Course ;
      ]
    ) ;
