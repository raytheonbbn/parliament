  rdfs:range ub:Course ;
.
ub:teachingAssistantOf
  a owl:ObjectProperty ;
  rdfs:domain ub:TeachingAssistant ;
  rdfs:label "is a teaching assistant for" ;
  rdfs:range ub:Course ;
.
ub:telephone
  a owl:DatatypeProperty ;
  rdfs:domain ub:Person ;
  rdfs:label "telephone number" ;
.
ub:tenured
  a owl:ObjectProperty ;
  rdfs:domain ub:Professor ;
  rdfs:label "is tenured:" ;
.
ub:title
  a owl:DatatypeProperty ;
  rdfs:domain ub:Person ;
  rdfs:label "title" ;
.
ub:undergraduateDegreeFrom
  a owl:ObjectProperty ;
  rdfs:domain ub:Person ;
  rdfs:label "has an undergraduate degree from" ;
