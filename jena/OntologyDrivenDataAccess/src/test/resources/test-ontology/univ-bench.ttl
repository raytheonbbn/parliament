# baseURI: http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl
# prefix: ub

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ub: <http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl>
  a owl:Ontology ;
  rdfs:comment "An university ontology for benchmark tests" ;
  rdfs:label "Univ-bench Ontology" ;
  owl:versionInfo "univ-bench-ontology-owl, ver April 1, 2004" ;
.
ub:AdministrativeStaff
  a owl:Class ;
  rdfs:label "administrative staff worker" ;
  rdfs:subClassOf ub:Employee ;
.
ub:Article
  a owl:Class ;
  rdfs:label "article" ;
  rdfs:subClassOf ub:Publication ;
.
ub:AssistantProfessor
  a owl:Class ;
  rdfs:label "assistant professor" ;
  rdfs:subClassOf ub:Professor ;
.
ub:AssociateProfessor
  a owl:Class ;
  rdfs:label "associate professor" ;
  rdfs:subClassOf ub:Professor ;
.
ub:Book
  a owl:Class ;
  rdfs:label "book" ;
  rdfs:subClassOf ub:Publication ;
.
ub:Chair
  a owl:Class ;
  rdfs:label "chair" ;
  rdfs:subClassOf ub:Professor ;
  owl:intersectionOf (
      ub:Person
      [
        a owl:Restriction ;
        owl:onProperty ub:headOf ;
        owl:someValuesFrom ub:Department ;
      ]
    ) ;
.
ub:ClericalStaff
  a owl:Class ;
  rdfs:label "clerical staff worker" ;
  rdfs:subClassOf ub:AdministrativeStaff ;
.
ub:College
