  rdfs:subClassOf ub:Organization ;
.
ub:Schedule
  a owl:Class ;
  rdfs:label "schedule" ;
.
ub:Software
  a owl:Class ;
  rdfs:label "software program" ;
  rdfs:subClassOf ub:Publication ;
.
ub:Specification
  a owl:Class ;
