@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sgen:  <http://jena.hpl.hp.com/2003/04/schemagen#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

[
	a                        sgen:Config ;
	sgen:classnamesuffix     "" ;
	sgen:encoding            "TURTLE" ;
	sgen:noindividuals       false ;
	sgen:ontology            false ;
	sgen:owl                 true ;
	sgen:package             "com.bbn.packaged_ontology" ;
	sgen:strictIndividuals   true ;
	sgen:uppercase           false ;

	sgen:classSection        "\n    // +++++++++++++++   Classes       +++++++++++++++\n" ;
	sgen:datatypeSection     "\n    // +++++++++++++++   Datatypes     +++++++++++++++\n" ;
	sgen:individualsSection  "\n    // +++++++++++++++   Individuals   +++++++++++++++\n" ;
	sgen:propSection         "\n    // +++++++++++++++   Properties    +++++++++++++++\n" ;

	# This header should be declared here, but schemagen interprets it as a boolean
	# when it read it from the config file, so we supply this as a command-line
	# parameter instead.
#	sgen:header              """/*
