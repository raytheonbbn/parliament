@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sgen:  <http://jena.hpl.hp.com/2003/04/schemagen#> .

[
	a                        sgen:Config ;
	sgen:classnamesuffix     "" ;
	sgen:noindividuals       false ;
	sgen:ontology            false ;
	sgen:owl                 true ;
	sgen:strictIndividuals   true ;
	sgen:uppercase           false ;

	sgen:classSection        "\n    // +++++++++++++++   Classes       +++++++++++++++\n" ;
	sgen:datatypeSection     "\n    // +++++++++++++++   Datatypes     +++++++++++++++\n" ;
	sgen:individualsSection  "\n    // +++++++++++++++   Individuals   +++++++++++++++\n" ;
	sgen:propSection         "\n    // +++++++++++++++   Properties    +++++++++++++++\n" ;

	sgen:header              """// Auto-generated by jena.schemagen on %date%
	
%package%

%imports%""" ;
] .
